{
  "name": "moa-cli-core",
  "description": "Mother of All CLI for AI automation",
  "version": "0.0.0",
  "author": "Chintan Shah",
  "bin": {
    "moa": "./bin/run.js"
  },
  "bugs": "https://github.com/Promact/moa-cli-core/issues",
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "axios": "^1.13.4",
    "bottleneck": "^2.19.5",
    "cli-table3": "^0.6.5",
    "keytar": "^7.9.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/keytar": "^4.4.2",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/Promact/moa-cli-core",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "moa",
    "dirname": "moa",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "config": {
        "description": "Manage CLI configuration and profiles"
      },
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "Promact/moa-cli-core",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}